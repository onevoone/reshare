{
	"version": 2,
	"builds": [
		{ "src": "dist/**", "use": "@now/static" }
	],
	"routes": [
		{ "src": "/(.*)", "dest": "dist/$1" }
	],
	"env": {
		"NODE_ENV": "production",
		"API_KEY": "",
		"AUTH_DOMAIN": "",
		"DATABASE_URL": "",
		"PROJECT_ID": "",
		"STORAGE_BUCKET": "",
		"MESSAGING_SENDER_ID": "",
		"APP_ID": "",
		"MEASUREMENT_ID": ""
	}
}